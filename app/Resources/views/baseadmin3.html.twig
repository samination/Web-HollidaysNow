<!DOCTYPE html>
<html lang="en">
<head>
    <title>Holidays Now</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Travello template project">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('logint.css') }}"  >
        <link rel="stylesheet" type="text/css" href="{{ asset('test.css') }}"  >
        <link rel="stylesheet" href="{{ asset ('taswira.css')}}">
        <link rel="stylesheet" type="text/css" href="{{ asset('travello/styles/bootstrap4/bootstrap.min.css') }}">
        <link href="{{ asset('travello/plugins/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet') }}" type="text/css">
        <link rel="stylesheet" type="text/css" href="{{ asset('travello/plugins/OwlCarousel2-2.2.1/owl.carousel.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('travello/plugins/OwlCarousel2-2.2.1/owl.theme.default.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('travello/plugins/OwlCarousel2-2.2.1/animate.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('travello/styles/main_styles.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('travello/styles/responsive.css') }}">
        <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.css">

    {% endblock %}
    <link rel="stylesheet" href="{{ asset('https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css') }}">
</head>


<div class="super_container">

    <!-- Header -->
    {% block header %}

        <header class="header">

            <div class="container">
                <div class="row">
                    <div class="col">
                        <div class="header_content d-flex flex-row align-items-center justify-content-start">
                            <div class="header_content_inner d-flex flex-row align-items-end justify-content-start">
                                <div class="logo"><a  >Holidays Now</a></div>
                                <nav class="main_nav">
                                    <ul class="d-flex flex-row align-items-start justify-content-start">
                                        <li class="active"><a >Home</a></li>
                                        <li class="dropdown">
                                            <a href="#" data-toggle="dropdown" style=" color: #333333; text-decoration: none;">
                                                Vols
                                            </a>
                                            <ul class="dropdown-menu">
                                                <li>
                                                    <ul>
                                                        <li>
                                                            <a href="{{ path('ajouter_vol') }}"  style="color: black;">
                                                                <i class="ti-link" ></i> Ajouter Vol</a>
                                                        </li>
                                                        <li>
                                                            <a href="{{ path('afficher_par_id_vol') }}" style="color: black;"  >
                                                                <i class="ti-link"></i> Gérer Vol</a>
                                                        </li>

                                                    </ul>
                                                </li>

                                            </ul >
                                        </li>
                                        <li class="dropdown">
                                            <a href="#" data-toggle="dropdown" style=" color: #333333; text-decoration: none;">
                                                Restaurants
                                            </a>
                                            <ul class="dropdown-menu">
                                                <li>
                                                    <ul>
                                                        {% if is_granted("ROLE_AGENT") %}
                                                        <li>

                                                            <a href="{{ path('ajouter') }}"  style="color: black;">
                                                                <i class="ti-link" ></i> Ajouter Restaurant</a>
                                                        </li>
                                                        {% endif %}
                                                        <li>
                                                            <a href="{{ path('afficher') }}" style="color: black;"  >
                                                                <i class="ti-link"></i> Liste Restaurants</a>
                                                        </li>

                                                    </ul>
                                                </li>

                                            </ul>
                                        </li>
                                        <li><a >Transport</a></li>
                                        <li class="dropdown">
                                            <a href="#" data-toggle="dropdown" style=" color: #333333; text-decoration: none;">
                                                Voyage Organisé
                                            </a>
                                        <ul class="dropdown-menu">
                                            <li>
                                                <ul>
                                                    <li>
                                                        <a href="{{ path('voyageorganise_ajout') }}"  style="color: black;">
                                                            <i class="ti-link" ></i> Ajouter Voyage Organisé</a>
                                                    </li>
                                                    <li>
                                                        <a href="{{ path('voyageorganise_affiche') }}" style="color: black;"  >
                                                            <i class="ti-link"></i> Gérer Voyage Organisé</a>
                                                    </li>

                                                </ul>
                                            </li>

                                        </ul >
                                        </li>


                                        <li class="dropdown">
                                            <a href="#" data-toggle="dropdown" style=" color: #333333; text-decoration: none;">
                                                Activité
                                            </a>
                                            <ul class="dropdown-menu">
                                                <li>
                                                    <ul>
                                                        <li>
                                                            <a href="{{ path('ajouter_activite') }}"  style="color: black;">
                                                                <i class="ti-link" ></i> Ajouter Activité</a>
                                                        </li>
                                                        <li>
                                                            <a href="{{ path('afficher_par_id') }}" style="color: black;"  >
                                                                <i class="ti-link"></i> Gérer Activité</a>
                                                        </li>

                                                    </ul>
                                                </li>

                                            </ul >
                                        </li>


                                        <li class="dropdown">
                                            <a href="#" data-toggle="dropdown" style=" color: #333333; text-decoration: none;">
                                                Activité
                                            </a>
                                            <ul class="dropdown-menu">
                                                <li>
                                                    <ul>
                                                        <li>
                                                            <a href="{{ path('ajouter_activite') }}"  style="color: black;">
                                                                <i class="ti-link" ></i> Ajouter Activité</a>
                                                        </li>
                                                        <li>
                                                            <a href="{{ path('afficher_par_id') }}" style="color: black;"  >
                                                                <i class="ti-link"></i> Gérer Activité</a>
                                                        </li>

                                                    </ul>
                                                </li>

                                            </ul >
                                        </li>


                                        <div align="right" class="dropdown">
                                            <button class="btn btn-primary dropdown-toggle" type="button" data-toggle="dropdown">Mon Compte
                                            </button>

                                            <ul class="dropdown-menu">
                                                {% if app.user %}
                                                    <a href="{{ path('fos_user_profile_show') }}">{{ app.user.username }}</a>
                                                    <a href="{{ path('fos_user_security_logout') }}">Deconnecte</a>
                                                {% else %}
                                                    <a href="{{ path('fos_user_security_login') }}">Login</a>
                                                    <a href="{{ path('fos_user_registration_register') }}">Inscription</a>
                                                {% endif %}
                                            </ul>
                                        </div>
                                    </ul>

                                </nav>


                                <!-- Hamburger -->

                                <div class="hamburger ml-auto">
                                    <i class="fa fa-bars" aria-hidden="true"></i>
                                </div>

                            </div>
                        </div>
                    </div>

                </div>

            </div>

            <div class="header_social d-flex flex-row align-items-center justify-content-start">
                <ul class="d-flex flex-row align-items-start justify-content-start">
                    <li><a href="#"><i class="fa fa-pinterest" aria-hidden="true"></i></a></li>
                    <li><a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
                    <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                    <li><a href="#"><i class="fa fa-dribbble" aria-hidden="true"></i></a></li>
                    <li><a href="#"><i class="fa fa-behance" aria-hidden="true"></i></a></li>
                    <li><a href="#"><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
                </ul>
            </div>

        </header>
    {% endblock %}


    {% block contentadmin %}

    {% endblock %}
    <!-- Footer -->



</div>
<script src="{{ asset('https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js') }}"></script>
<script src="{{ asset('https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js') }}"></script>
<script src="{{ asset('travello/js/jquery-3.2.1.min.js') }}"></script>
<script src="{{ asset('travello/styles/bootstrap4/popper.js') }}"></script>
<script src="{{ asset('travello/styles/bootstrap4/bootstrap.min.js') }}"></script>
<script src="{{ asset('travello/plugins/OwlCarousel2-2.2.1/owl.carousel.js') }}"></script>
<script src="{{ asset('travello/plugins/Isotope/isotope.pkgd.min.js') }}"></script>
<script src="{{ asset('travello/plugins/scrollTo/jquery.scrollTo.min.js') }}"></script>
<script src="{{ asset('travello/plugins/easing/easing.js') }}"></script>
<script src="{{ asset('travello/plugins/parallax-js-master/parallax.min.js') }}"></script>
<script src="{{ asset('travello/js/custom.js') }}"></script>

<script src="{{ asset('javascript.js') }}"></script>
<script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.js"></script>
<script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/plug-ins/1.10.16/i18n/French.json"></script>


{% block Scripts %}
{% endblock %}
</html>